{
  "manifest_version": 2,
  "name": "Resposta aí",
  "description": "Desabilitar o blur do site responde aí",
  "version": "1.000",
  "default_locale": "en",
  "permissions": [
    "storage",
    "tabs",
    "contextMenus",
    "<all_urls>",
    "webRequest",
    "webRequestBlocking",
    "notifications"
  ],
  "icons": {
    "16": "data/icons/16.png",
    "32": "data/icons/32.png",
    "48": "data/icons/48.png",
    "128": "data/icons/128.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {}
}
